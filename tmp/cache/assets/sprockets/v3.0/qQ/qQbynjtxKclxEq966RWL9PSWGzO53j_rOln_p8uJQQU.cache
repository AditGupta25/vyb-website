I"¹(function() {
  window.App || (window.App = {});

  App.init = function() {
    if ($('.navbar').length > 0) {
      $(window).on('scroll', function() {
        if ($(window).scrollTop() > 50) {
          return $('.navbar').addClass('scrolled');
        } else {
          return $('.navbar').removeClass('scrolled');
        }
      });
    }
    if ($('.ico').length > 0) {
      $('#attentionModal').modal('show');
    }
    if ($('.home').length > 0) {
      $('video')[0].play();
    }
    $(document).on('click', function() {
      return $('.collapse').collapse('hide');
    });
    if ($('.home').length > 0) {
      window.sr = ScrollReveal();
      sr.reveal('.reveal-jumbotron', {
        duration: 800
      });
      sr.reveal('.reveal-card-left', {
        duration: 1200,
        origin: 'left',
        distance: '120px'
      });
      sr.reveal('.reveal-card-right', {
        duration: 1200,
        origin: 'right',
        distance: '120px'
      });
      return sr.reveal('.reveal-card-up', {
        duration: 1200,
        origin: 'bottom',
        distance: '120px'
      });
    }
  };

  $(document).on("turbolinks:load", function() {
    return App.init();
  });

}).call(this);
